{% extends 'office/layout.twig' %}

{% block body %}
    <a href="/"> <- Back</a>
    <h1>Office</h1>

    <h5>{{ user.email }}</h5>
    <h5>{{ user.name }}</h5>
    <h5>{{ user.surname }}</h5>
    <h5>{{ user.phone }}</h5>
    <h5>{{ user.access }}</h5>

    <form ref='uploadForm'
          id='uploadForm'
          action='/api/files/'
          method='post'
          encType="multipart/form-data"
    >

        <input type="file" name="sampleFile" />
        <input type='submit' value='Upload!' />
    </form>

{% endblock body %}